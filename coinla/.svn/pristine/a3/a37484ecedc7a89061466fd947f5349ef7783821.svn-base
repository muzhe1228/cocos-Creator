.$route.params.uuid\n        }).then((res) => {\n          let resData = res.data.data\n          if(res.data.code == 0){\n            this.awardsList = resData\n            alert(JSON.stringify(this.awardsList))\n          }else{\n            this.$toast({message: res.data.message, duration: 1500})\n          }\n        }).catch((err) => {\n          this.$toast({message: '失败', duration: 1500})\n        })\n      },\n      getGroupAward (infoId){\n          alert(JSON.stringify({\n            uuid: this.$route.params.uuid,\n            awardsName: awardsName,\n            infoId: infoId,\n            headersToken: this.$route.params.headersToken\n          }))\n        this.$router.push({name: 'getGroupAward', params: {\n          uuid: this.$route.params.uuid,\n          awardsName: awardsName,\n          infoId: infoId,\n          headersToken: this.$route.params.headersToken\n        }})\n      },\n    }\n  }\n</script>\n\n<style scoped lang=\"stylus\">\n  .group_award{\n    min-height: 100vh\n    padding: 0.88rem 0.32rem 0.46rem\n    background: url(/static/images/group/group_award_bac.png) no-repeat\n    background-size: 100% 100%\n    overflow: hidden\n   .group_award_switch_content{\n      width: 100%\n      height: 10.4rem\n      background-color: #e4faff\n      border-radius: 0.2rem\n      overflow: hidden\n      .title_wrap{\n        display: flex\n        height: 0.82rem\n        line-height: 0.82rem\n        background-color: #afedfb\n        p{\n          width: 33.33%\n          text-align: center\n          color: #0b73b9\n          font-size: 0.28rem\n        }\n      }\n      .group_detail_wrap{\n        overflow: hidden\n        .detail_content_wrap{\n          position: relative\n          height: 9.78rem\n          overflow-x: hidden\n          overflow-y: scroll\n          background-color: #e4faff\n          .detail_content{\n            display: flex\n            height: 0.88rem\n            line-height: 0.88rem\n            overflow: hidden\n            background-color: #effbfd\n            p{\n              width: 33.33%\n              text-align: center\n              color: #f78353\n              font-size: 0.28rem\n            }\n            .time{\n              color: #49abc2\n              font-size: 0.24rem\n            }\n            em{\n              width: 0.16rem\n              height: 0.16rem\n              position: absolute\n              right: 0.3rem\n              top: 0.42rem\n              border-left: 1px solid #49abc2\n              border-bottom: 1px solid #49abc2\n              -webkit-transform: translate(0,-50%) rotate(-135deg)\n              transform: translate(0,-50%) rotate(-135deg)\n            }\n          }\n          .detail_content:nth-child(even){\n            background-color: #e4faff\n          }\n        }\n      }\n    }\n  }\n</style>\n\n\n\n// WEBPACK FOOTER //\n// src/components/groupAward.vue","var render = function () {var _vm=this;var _h=_vm.$createElement;var _c=_vm._self._c||_h;return _c('div',{staticClass:\"group_award\"},[_c('div',{staticClass:\"group_award_main\"},[_c('div',{staticClass:\"group_award_switch_content\"},[_vm._m(0),_vm._v(\" \"),_c('div',{staticClass:\"group_detail_wrap\"},[_c('div',{staticClass:\"detail_content_wrap\"},_vm._l((_vm.awardsList),function(item,index){return _c('div',{staticClass:\"detail_content\",on:{\"click\":function($event){_vm.getGroupAward(item.awardsName, item.infoId)}}},[_c('p',{staticClass:\"time\"},[_vm._v(\"\\n              \"+_vm._s(_vm._f(\"filterDate\")(item.updateDate))+\"\\n            \")]),_vm._v(\" \"),_c('p',{staticClass:\"group_number\"},[_c('span',[_vm._v(_vm._s(item.awardsName?item.awardsName:'0'))])]),_vm._v(\" \"),_c('p',{staticClass:\"my_number\"},[_vm._v(_vm._s(item.myReceiveState == 0? '未领取': item.myAwardsName)+\" >\")])])}))])])])])}\nvar staticRenderFns = [function () {var _vm=this;var _h=_vm.$createElement;var _c=_vm._self._c||_h;return _c('div',{staticClass:\"title_wrap\"},[_c('p',[_vm._v(\"