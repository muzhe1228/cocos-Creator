* {
  list-style: none;
  text-decoration: none;
  //font-family: Arial, HelveticaRe, "Helvetica Neue", 'Microsoft Yahei', '\5FAE\8F6F\96C5\9ED1', 'Hiragino Sans GB', '\5B8B\4F53' !important;
  box-sizing: border-box;
  margin: 0;
  padding: 0;
  //font-weight: 400;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  outline: none;
}

body {
  line-height: 1;
  -webkit-tap-highlight-color: transparent;
  min-width: 1190px;
  //font-family:  Arial, 'Times New Roman', HelveticaRe, "Helvetica Neue", "Microsoft YaHei Regular", "Microsoft YaHei", "微软雅黑", sans-serif !important
  background-color: #f5f5f5;
  color: @size-main;

}

a {
  color: @size-main;
}

#cnzz_stat_icon_1273761229 {
  position: fixed;
  left: 0;
  top: 0;
  z-index: -1;
  opacity: 0
}

.f-l {

  float: left
}

.f-r {

  float: right
}

.clear-f:after {
  content: "\0020";
  display: block;
  height: 0;
  clear: both
}

.clear-f {

  zoom: 1
}

.container {
  width: 1190px;
  margin: 0 auto;
  position: relative;
}

//width: calc(100% / 1.6);

.color-red {

  color: #f53846 !important
}

.color-green {

  color: #00b785 !important
}

.color-gray {

  color: #454545 !important
}

.color-star {

  color: #b5b5b5 !important
}

.color-starA {

  color: #debd63 !important
}

.goToUrl {
  z-index: 2
}

.AppDown {
  img {
    width: 324px;
    height: 180px
  }
}

.Href {
  cursor: pointer;
  text-decoration: underline;
  .transition(all .2s linear);
  &:hover {
    color: @main-color !important
  }
}

.aClick {
  cursor: pointer;
  color: #0d93ff;
  .flex;
  width: 100%;
  height: 100%;
  .align-items(center);
  .transition(all .2s linear);
  &:hover {
    text-decoration: underline
  }
}

input:-webkit-autofill {
  -webkit-box-shadow: 0 0 0 1000px white inset;
  -webkit-text-fill-color: #333
}

@keyframes SvgAm {
  0% {
    stroke-dasharray: 0, 1000
  }
  30% {
    stroke-dasharray: 150, 1000
  }
  60% {
    stroke-dasharray: 300, 1000
  }

  90% {
    stroke-dasharray: 450, 1000
  }
  100% {
    stroke-dasharray: 1000, 1000
  }
}

@keyframes hoverBook {
  0% {
    opacity: 0
  }
  30% {
    opacity: .25
  }
  60% {
    opacity: .5
  }
  90% {
    opacity: .75
  }
  100% {
    opacity: 1
  }
}

img[lazy=loading] {
  //your code
}

img[lazy=loaded] {
  //your code
  animation: fade 0.5s
}

@keyframes fade {
  0% {
    opacity: 0
  }
  100% {
    opacity: 1
  }
}

.noDataImg {
  position: relative;
  left: 50%;
  .transform(translateX(-50%));
  max-width: 200px;
  padding: 20px;
}

.modeWrap {
  width: 100vw;
  height: 100vh;
  background-color: rgba(0, 0, 0, .3);
  position: fixed;
  left: 0;
  top: 0;
  .config {
    width: 300px;
    height: 160px;
    background-color: #fff;
    box-shadow: 0 0 15px rgba(0, 0, 0, .1);
    position: absolute;
    left: 50%;
    top: 120px;
    margin-left: -150px;
    border-radius: 4px;
  }

  .config-info {
    height: 100px;
    width: 100%;
    position: relative;
  }

  .config-info p {
    width: 100%;
    font-size: 16px;
    line-height: 1.4;
    position: absolute;
    text-align: center;
    vertical-align: middle;
    top: 50%;
    -webkit-transform: translateY(-50%);
    -moz-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    -o-transform: translateY(-50%);
    transform: translateY(-50%);
  }

  .config-btn {
    margin-top: 10px;
    height: 50px;
    text-align: center;
  }

  .config-btn button {
    cursor: pointer;
    margin: 2px 20px;
    width: 80px;
    height: 36px;
    border: 1px solid #debd63;
    background-color: #debd63;
    color: #fff;
    border-radius: 4px;
    outline: none;
  }

  .config-btn button:nth-child(2) {

    border: 1px solid #dedede;
    background-color: #dedede;
  }
}


