 = res.data.data\n          if(res.data.code == 0){\n            this.awardsList = resData\n          }else{\n            this.$toast({message: res.data.message, duration: 1500})\n          }\n        }).catch((err) => {\n          this.$toast({message: '失败', duration: 1500})\n        })\n      }\n    }\n  }\n</script>\n\n<style scoped lang=\"stylus\">\n  .group_award{\n    min-height: 100vh\n    padding: 0.88rem 0.32rem 0.46rem\n    background: url(/static/images/group/group_award_bac.png) no-repeat\n    background-size: 100% 100%\n    overflow: hidden\n   .group_award_switch_content{\n      width: 100%\n      height: 10.4rem\n      background-color: #e4faff\n      border-radius: 0.2rem\n      overflow: hidden\n      .title_wrap{\n        display: flex\n        height: 0.82rem\n        line-height: 0.82rem\n        background-color: #afedfb\n        p{\n          width: 33.33%\n          text-align: center\n          color: #0b73b9\n          font-size: 0.28rem\n        }\n      }\n      .group_detail_wrap{\n        overflow: hidden\n        .detail_content_wrap{\n          height: 9.78rem\n          overflow-x: hidden\n          overflow-y: scroll\n          background-color: #e4faff\n          .detail_content{\n            position: relative\n            display: flex\n            height: 0.88rem\n            line-height: 0.88rem\n            overflow: hidden\n            background-color: #effbfd\n            p{\n              width: 33.33%\n              text-align: center\n              color: #f78353\n              font-size: 0.28rem\n            }\n            .time{\n              color: #49abc2\n              font-size: 0.24rem\n            }\n            em{\n              width: 0.16rem\n              height: 0.16rem\n              position: absolute\n              right: 0.3rem\n              top: 0.42rem\n              border-left: 1px solid #49abc2\n              border-bottom: 1px solid #49abc2\n              -webkit-transform: translate(0,-50%) rota