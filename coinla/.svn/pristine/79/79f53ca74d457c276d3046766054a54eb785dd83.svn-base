         input {\n            line-height: normal\n            width: 3.36rem;\n            position: absolute;\n            font-size: 0.3rem\n            background-color: #fff1da\n            text-align: center\n            border: none\n            color: #333\n            border-radius: 0.12rem\n            right: 0;\n            top: 0;\n            height: 0.82rem\n            overflow: hidden;\n            &:focus {\n              border: 1px solid #fff1da;\n              background-color: #fff;\n            }\n          }\n          label {\n            position: absolute;\n            left: 0;\n            top: 0;\n            z-index: 1;\n            width: 1.88rem;\n            img {\n              height: 0.82rem\n              width: 1.88rem\n              border-radius: 0.12rem;\n            }\n          }\n        }\n      }\n    }\n    .yd {\n      width: 100%\n      height: 100%\n      position: fixed\n      background-color: rgba(0, 0, 0, .5)\n      left: 0\n      top: 0\n      z-index: 9999\n      img {\n        position: absolute\n        right: 10px\n        width: 6.1rem\n        top: 0\n      }\n    }\n  }\n</style>\n\n\n\n// WEBPACK FOOTER //\n// src/components/index.vue","import {getCoinlaSingle} from 'assets/js/httpAll'\n// 表单验证规则\nlet validates = {\n  account(val) {//手机号正则\n    return /^[1][3,4,5-9][0-9]\\d{8}$/.test(val)\n  },\n  captcha(val) {//验证码正则\n    return /^[0-9]{6}$/.test(val)\n  },\n  passWd(val) {//密码正则\n    return /^(\\w+){6,16}$/.test(val)\n  },\n  nick(val) {\n    return /^[a-zA-Z\\d\\_\\u2E80-\\u9FFF]{3,10}$/.test(val)\n  }\n}\n// 表单验证提示\nlet validateHandles = {\n  account(value) {\n    if (!value.trim()) {\n      return '请输入手机号!'\n    } else if (!validates.account(value)) {\n      return '手机号格式错误!'\n    } else {\n      return false\n    }\n  },\n  captcha(value) {\n    if (!value.trim()) {\n      return '请输入验证码!'\n    } else if (!validates.captcha(value)) {\n      return '验证码为6位数字!'\n    }else {\n      return false\n    }