AAA,QAAAJ,EAAA","file":"static/js/5.d4f92df6e7b1e110fe02.js","sourcesContent":["import {getCoinlaSingle} from 'assets/js/httpAll'\n// 表单验证规则\nlet validates = {\n  account(val) {//手机号正则\n    return /^[1][3,4,5-9][0-9]\\d{8}$/.test(val)\n  },\n  captcha(val) {//验证码正则\n    return /^[0-9]{6}$/.test(val)\n  },\n  passWd(val) {//密码正则\n    return /^(\\w+){6,16}$/.test(val)\n  },\n  nick(val) {\n    return /^[a-zA-Z\\d\\_\\u2E80-\\u9FFF]{3,10}$/.test(val)\n  }\n}\n// 表单验证提示\nlet validateHandles = {\n  account(value) {\n    if (!value.trim()) {\n      return '请输入手机号!'\n    } else if (!validates.account(value)) {\n      return '手机号格式错误!'\n    } else {\n      return false\n    }\n  },\n  captcha(value) {\n    if (!value.trim()) {\n      return '请输入验证码!'\n    } else if (!validates.captcha(value)) {\n      return '验证码为6位数字!'\n    }else {\n      return false\n    }\n  },\n  passwd(rule, value, callback) {\n    if (!value.trim()) {\n      callback(new Error('请输入密码!'))\n    } else if (!validates.passWd(value)) {\n      callback(new Error('6-16位数字，字母，下划线!'))\n    } else {\n      callback()\n    }\n  },\n  nick(rule, value, callback) {\n    if (!value.trim()) {\n      callback(new Error('请输入用户名!'))\n    } else if (!validates.nick(value)) {\n      callback(new Error('长度在3-10个字符!'))\n    } else {\n      callback()\n    }\n  }\n}\nexport {\n  validates,\n  validateHandles\n}\n\n// 获取验证码\nexport function valideCode(obj, val, type) {\n  const waitTime = 60\n  let time = waitTime\n  if (obj.isClick) {\n    obj.isClick = false\n    if (!val.trim()) {\n      obj.isClick = true\n      obj.$message.error('请输入手机号')\n    } else if (!validates.account(val)) {\n      obj.isClick = true\n      obj.$message.error('手机号格式不正确')\n    } else {\n      getCoinlaSingle('/captcha/sms', {account: val, type: type})\n        .then((res) => {\n          let code = res.data.code\n          if (code === 0) {\n            this.$message({\n              message: '验证码发送成功，请注意查收！',\n              type: 'success',\n              duration: 1000\n            })\n            obj.captchaSize = time + 'S后重新发送'\n            obj.isClick = false\n            obj.$refs.send.parentNode.style.backgroundColor = '#ccc'\n            obj.$refs.send.parentNode.style.borderColor = '#ccc'\n            obj.$refs.send.parentNode.style.cursor = 'not-allowed'\n            obj.timer = setInterval(function () {\n              obj.captchaSize = (--time) + 'S后重新发送'\n              if (time === 0) {\n                clearInterval(obj.timer)\n                obj.captchaSize = '重新发送'\n                obj.$refs.send.parentNode.style.backgroundColor = '#e2bd51'\n                obj.$refs.send.parentNode.style.borderColor = '#e2bd51'\n                obj.$refs.send.parentNode.style.cursor = 'pointer'\n                obj.isClick = true\n                time = waitTime\n              }\n            }, 1000)\n          } else {\n            obj.isClick = true\n            obj.$message({\n              message: res.data.message,\n              type: 'error',\n              duration: 1000\n            })\n          }\n        }).catch((err) => {\n        obj.isClick = true\n        obj.$message({\n          message: err.message,\n          type: 'error',\n          duration: 1000\n        })\n      })\n    }\n  }\n}\n\n\n\n// WEBPACK FOOTER //\n// ./src/assets/js/reg.js","<template>\n  <div class=\"indexWrap\" id=\"canvasImg\">\n    <div class=\"fixed\">\n      <div class=\"fixed-l\">\n        <p class=\"fixed-logo\"><img src=\"/static/images/download/app.png\" alt=\"\"></p>\n        <p class=\"size\">\n          <b>CoinLa&nbsp;&nbsp;考拉行情</b>\n          <span>聚合全球行情&nbsp;&nbsp;挖掘数据价值</span>\n        </p>\n      </div>\n      <a href=\"\" class=\"fixed-r\">\n        下载APP\n      </a>\n    </div>\n    <div class=\"banner downloadApp\">\n      <img src=\"/static/images/download/banner01.jpg\" alt=\"\">\n    </div>\n    <p class=\"logo\">\n      <img src=\"/static/images/download/logo.png\" alt=\"\">\n    </p>\n    <div class=\"indexForm\">\n      <p><input type=\"text\" v-model=\"register.account\"\n                @change=\"changeInp('account')\"\n                @input=\"checkEmpty(register.account,'account')\" placeholder=\"输入手机号\"></p>\n      <p class=\"form-gulp1\"><input type=\"text\" v-model=\"register.captcha\"\n                                   @change=\"changeInp('captcha')\"\n                                   @input=\"checkEmpty(register.account,'captcha')\" placeholder=\"输入验证码\">\n        <button class=\"captcha\"><label\n          for=\"codeImg\">获取验证码</label></button>\n      </p>\n    </div>\n    <button @click=\"registerHandle\" :disabled=\"!registerStatus\" class=\"submitBtn\">立即注册考拉行情</button>\n    <ul class=\"botSize\">\n      <li>活动说明：</li>\n      <li>1.&nbsp;在考拉行情APP的推荐有币页面，点击立即邀请好友，即可分享您的邀请链接给好友。</li>\n      <li>2.&nbsp;若新用户通过您的邀请码或邀请链接注册成功并下载登录（此新用户称为P1），每邀请一个P1，您可获得100 CLB，无邀请上限。</li>\n      <li>3.&nbsp;若新用户通过P1的邀请码或邀请链接注册成功并下载登录（此新用户称为P2），每邀请一个P2，您可获得50 CLB，无邀请上限。</li>\n      <li>4.&nbsp;推荐有币的活动，仅限手机号注册用户参与。一个设备号仅限注册两个考拉行情账号。被邀请好友注册安装后即可获得奖励。</li>\n      <li>5.&nbsp;其他未尽事宜请联系客服微信：KAOLAhq123</li>\n      <li>6.&nbsp;考拉行情保留对此活动的解释权,并有权随时对上述细则进行调整。</li>\n    </ul>\n    <div class=\"fixedMode\">\n      <div class=\"fixedWrap\">\n        <div class=\"fixedTop\">\n          <img class=\"fixedImg\" src=\"/static/images/download/img_right.png\" alt=\"\">\n          <p class=\"fixedIcon\">\n            <img class=\"iconWrap\" src=\"/static/images/download/icon_circle_xhdpi.png\" alt=\"\">\n            <img class=\"iconInfo\" src=\"/static/images/download/icon_right.png\" alt=\"\">\n          </p>\n          <p class=\"fixedSize\">注册成功</p>\n        </div>\n        <div class=\"fixedBot\">\n          <p class=\"fixedBotSize\">恭喜获得200CLB<br><br>下载App获得更多CLB</p>\n          <a href=\"\" class=\"fixedBotBtn\">立即下载</a>\n        </div>\n      </div>\n    </div>\n    <div class=\"modeShade\">\n      <div class=\"mode\">\n        <!--<p class=\"modeSize\">请输入验证</p>-->\n        <p class=\"code-gulp\"><input type=\"text\" id=\"codeImg\" placeholder=\"请输入计算结果\">\n          <label for=\"codeImg\" class=\"codeImg\"><img src=\"\" alt=\"\"></label>\n\n        </p>\n        <button class=\"verify\" disabled>发送验证码</button>\n      </div>\n    </div>\n    <img @click=\"rou\" :src=\"codeUrl\" alt=\"\">\n    <p class=\"yd\"><img src=\"/static/images/download/yd.png\" alt=\"\"></p>\n    <p class=\"tishi\"></p>\n  </div>\n</template>\n\n<script>\n  import {validates, validateHandles} from 'assets/js/reg'\n  import {getCoinlaSingle} from 'assets/js/httpAll'\n\n  export default {\n    data() {\n      return {\n        registerStatus: false,\n        register: {\n          account: '',\n          captcha: '',\n          client: 0,\n          recommendCode: this.$route.query.inviteCode\n        },\n        codeUrl: ''\n      }\n    },\n    mounted() {\n      console.log(this.register)\n      // this.$nextTick(function () {\n      //   this.canvasHandle()\n      // })\n    },\n    methods: {\n      registerHandle() {\n        getCoinlaSingle('/user/login/registerOrLogin', this.register).then((res) => {\n          if (res.data.code === 0) {\n          } else {\n            this.$toast({message: res.data.message, duration: 1500})\n          }\n          this.codeUrl = `https://api.coinla.com/captcha/verify/code?account=${this.register.account}`\n        }).catch((err) => {\n          this.$toast({message: err.message, duration: 1500})\n        })\n      },\n      changeInp(type) {\n        if (type == 'account') {\n          if (validateHandles.account(this.register.account)) {\n            this.$toast({message: validateHandles.account(this.register.account), duration: 1500})\n          }\n        } else if (type == 'captcha') {\n          if (validateHandles.captcha(this.register.captcha)) {\n            this.$toast({message: validateHandles.captcha(this.register.captcha), duration: 1500})\n          }\n        }\n      },\n      rou() {\n        this.cod